<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.rongcapital.mkt.dao.ImportDataHistoryDao">

	<!-- 所有查询列 -->
	<sql id="QUERY_COLUMN_LIST">
		<![CDATA[ id,import_start_time,import_end_time,total_rows,legal_rows,success,source,email_count,contact_count,mobile_count,qq_count]]>
	</sql>

	<!-- 查询列来源表-->
	<sql id="QUERY_FROM_TABLE"><![CDATA[FROM import_data_history]]></sql>
	
	<!-- 全部条件 -->
	<sql id="QUERY_WHERE_CLAUSE">
		<where>
		    	<if test="id != null and id != ''"><![CDATA[id = #{id} AND ]]></if>
		    	<if test="import_start_time != null and import_start_time != ''"><![CDATA[import_start_time = #{import_start_time} AND ]]></if>
		    	<if test="import_end_time != null and import_end_time != ''"><![CDATA[import_end_time = #{import_end_time} AND ]]></if>
		    	<if test="total_rows != null and total_rows != ''"><![CDATA[total_rows = #{total_rows} AND ]]></if>
		    	<if test="legal_rows != null and legal_rows != ''"><![CDATA[legal_rows = #{legal_rows} AND ]]></if>
		    	<if test="success != null and success != ''"><![CDATA[success = #{success} AND ]]></if>
		    	<if test="source != null and source != ''"><![CDATA[source = #{source} AND ]]></if>
		    	<if test="email_count != null and email_count != ''"><![CDATA[email_count = #{email_count} AND ]]></if>
		    	<if test="contact_count != null and contact_count != ''"><![CDATA[contact_count = #{contact_count} AND ]]></if>
		    	<if test="mobile_count != null and mobile_count != ''"><![CDATA[mobile_count = #{mobile_count} AND ]]></if>
		    	<if test="qq_count != null and qq_count != ''"><![CDATA[qq_count = #{qq_count}]]></if>
		</where>
	</sql>
	
	<!-- 智能排序与分页 -->
	<sql id="QUERY_ORDER_LIMIT_CONDTION">
		<if test="orderField != null and orderField != '' and orderFieldType != null and orderFieldType != ''"><![CDATA[ORDER BY ${orderField} ${orderFieldType}]]></if>
		<if test="startIndex != null and startIndex &gt;= 0 and pageSize != null and pageSize &gt; 0"><![CDATA[LIMIT #{startIndex},#{pageSize}]]></if>	
	</sql>

	<!-- 更新列字段,只要不为NULL则更新,除开主键列 -->
	<sql id="UPDATE_COLUMN_SET">
		<set>
		    	<if test="id != null"><![CDATA[AND id = #{id},]]></if>
		    	<if test="import_start_time != null"><![CDATA[AND import_start_time = #{import_start_time},]]></if>
		    	<if test="import_end_time != null"><![CDATA[AND import_end_time = #{import_end_time},]]></if>
		    	<if test="total_rows != null"><![CDATA[AND total_rows = #{total_rows},]]></if>
		    	<if test="legal_rows != null"><![CDATA[AND legal_rows = #{legal_rows},]]></if>
		    	<if test="success != null"><![CDATA[AND success = #{success},]]></if>
		    	<if test="source != null"><![CDATA[AND source = #{source},]]></if>
		    	<if test="email_count != null"><![CDATA[AND email_count = #{email_count},]]></if>
		    	<if test="contact_count != null"><![CDATA[AND contact_count = #{contact_count},]]></if>
		    	<if test="mobile_count != null"><![CDATA[AND mobile_count = #{mobile_count},]]></if>
		    	<if test="qq_count != null"><![CDATA[AND qq_count = #{qq_count}]]></if>
		</set>
	</sql>

	<!-- 插入action记录 -->
	<insert id="insert" parameterType="cn.rongcapital.mkt.po.ImportDataHistory" useGeneratedKeys="true" keyProperty="id">
		<![CDATA[
			INSERT INTO import_data_history (id,import_start_time,import_end_time,total_rows,legal_rows,success,source,email_count,contact_count,mobile_count,qq_count)
			VALUES
			(
			  #{id},#{import_start_time},#{import_end_time},#{total_rows},#{legal_rows},#{success},#{source},#{email_count},#{contact_count},#{mobile_count},#{qq_count}
			)
		]]>
	</insert>
	
	<!-- 返回插入的编号,在事务开启状态下有效 -->
	<select id="lastSequence" resultType="int"><![CDATA[SELECT LAST_INSERT_ID() AS id]]></select>

	<!-- 修改记录通过主键 -->
	<update id="updateById" parameterType="cn.rongcapital.mkt.po.ImportDataHistory">
		<![CDATA[UPDATE import_data_history]]>
		<include refid="UPDATE_COLUMN_SET"/>
		<![CDATA[WHERE id = #{id}]]>
	</update>

	<!-- 查询,通过主键IN(array) -->
	<select id="selectListByIdList" parameterType="cn.rongcapital.mkt.po.ImportDataHistory" resultType="cn.rongcapital.mkt.po.ImportDataHistory">
		<![CDATA[SELECT]]>
		<include refid="QUERY_COLUMN_LIST"/>
		<include refid="QUERY_FROM_TABLE"/>
		<![CDATA[WHERE id IN]]>
		<foreach collection="list" item="id" open="(" separator="," close=")">
			<![CDATA[#{id}]]>
		</foreach>
	</select>

	<!-- 查询,通过条件 -->
	<select id="selectList" parameterType="cn.rongcapital.mkt.po.ImportDataHistory" resultType="cn.rongcapital.mkt.po.ImportDataHistory">
		<![CDATA[SELECT]]>
		<include refid="QUERY_COLUMN_LIST"/>
		<include refid="QUERY_FROM_TABLE"/>
		<include refid="QUERY_WHERE_CLAUSE"/>
		<include refid="QUERY_ORDER_LIMIT_CONDTION"/>
	</select>

	<!-- 总数查询,通过条件 -->
	<select id="selectListCount" parameterType="cn.rongcapital.mkt.po.ImportDataHistory" resultType="int">
		<![CDATA[SELECT COUNT(id) AS dataCount]]>
		<include refid="QUERY_FROM_TABLE"/>
		<include refid="QUERY_WHERE_CLAUSE"/>
	</select>
	
	<!-- 其它SQL语句 -->
	
	
</mapper>