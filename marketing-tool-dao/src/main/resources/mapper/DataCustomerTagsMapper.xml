	<select id="getAudiencesCount" parameterType="java.util.Map" resultType="int">
		<![CDATA[SELECT]]>
			count(id) as data_custom_tag_rows
		<include refid="QUERY_FROM_TABLE"/>
		<![CDATA[WHERE 1=1]]> 
				
