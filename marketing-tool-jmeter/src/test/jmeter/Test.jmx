          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="获取参数" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.io.BufferedReader;
import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Iterator;
import org.codehaus.jettison.json.*;

String userId = vars.get(&quot;user_id&quot;);
String userToken = props.get(&quot;user_token&quot;);
String ver = vars.get(&quot;ver&quot;);
String method = vars.get(&quot;method&quot;);


File jmeterBinFile = new File(&quot;&quot;);
String jmeterPath = jmeterBinFile.getCanonicalPath();
// 获取marketing-tool-jmeter的绝对路径
String filePath = jmeterPath.substring(0,jmeterPath.length()-17);
filePath = filePath + &quot;src/test/jmeter/config/requestParameter/&quot;+ method +&quot;.json&quot;;


// 输出读入的内容
String httpRequsetMethods = &quot;GET&quot;;
JSONObject parameterJson;
File file = new File(filePath);
String parameterStr = &quot;&quot;;
try {
            BufferedReader br = new BufferedReader(new InputStreamReader(new FileInputStream(file)));
            String data = null;
            while((data = br.readLine())!=null) {
                if(!data.trim().isEmpty()) {
                    parameterStr += data;
                }
            }
            parameterJson = new JSONObject(parameterStr);
            boolean httpRequsetMethodsIsNull = parameterJson.isNull(&quot;http_request&quot;);
            if(httpRequsetMethodsIsNull) {
                httpRequsetMethods = &quot;GET&quot;;
            } else {
                httpRequsetMethods = parameterJson.getString(&quot;http_request&quot;);
                httpRequsetMethods = httpRequsetMethods.toUpperCase();
                if(!&quot;POST&quot;.equals(httpRequsetMethods)){
                    httpRequsetMethods = &quot;GET&quot;;
                }
                parameterJson.remove(&quot;http_request&quot;);
            }
            parameterJson.put(&quot;user_id&quot;, userId);
            parameterJson.put(&quot;user_token&quot;, userToken);
            parameterJson.put(&quot;ver&quot;, ver);
        } catch (FileNotFoundException e) {
            System.out.println(e.getMessage());
        } catch (IOException e) {
            System.out.println(e.getMessage());
        } catch (JSONException e) {
            System.out.println(e.getMessage());
        }


// 设置GET参数
String GETParameter = &quot;&quot;;  
try {
	Iterator iterator = parameterJson.keys();
	while(iterator.hasNext()) {
		String key = iterator.next().toString();
		GETParameter = GETParameter + &quot;&amp;&quot; + key + &quot;=&quot; + parameterJson.getString(key);
	}
} catch (JSONException e) {
}    
if(!&quot;GET&quot;.equals(httpRequsetMethods)) {
	GETParameter = &quot;&quot;;
}
vars.put(&quot;GETRequest&quot;,GETParameter);
// 设置POST参数
String POSTParameter;
if(&quot;POST&quot;.equals(httpRequsetMethods)) {
//	System.out.println(parameterJson.toString());
     POSTParameter = parameterJson.toString();
} else {
	POSTParameter = &quot;&quot;;
}
vars.put(&quot;POSTRequest&quot;,POSTParameter);

// 设置http请求
vars.put(&quot;http_method&quot;,httpRequsetMethods);</stringProp>
